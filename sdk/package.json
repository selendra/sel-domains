{
  "name": "@selendra/sns-sdk",
  "version": "0.1.0",
  "description": "SDK for Selendra Naming Service (.sel domains)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  },
  "keywords": [
    "selendra",
    "sns",
    "domains",
    "web3",
    "blockchain",
    "viem"
  ],
  "author": "Selendra",
  "license": "MIT"
}
